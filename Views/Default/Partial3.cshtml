@using TravelTripProje.Models.Sınıflar
@model List<Blog>

<div class="padding">
    <h4> En Populer 10 Blog </h4>
    @{ int i = 0;}
    <ul class="list list-pad">
        @foreach (var x in Model)
        {
            <li class="list-bg"><span>@{i++;} @i.  </span><a href="/Blog/BlogDetay/@x.ID">@x.Baslik</a></li>
        }
    </ul>
    <a href="/Blog/Index" class="link1">  Tum Bloglari Gor  </a>
</div>
